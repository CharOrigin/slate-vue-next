{
  "private": true,
  "name": "slate-vue-packages",
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "url": "https://github.com/marsprince/slate-vue-next"
  },
  "scripts": {
    "build": "rollup --config ./config/rollup/rollup.config.js",
    "serve": "webpack-dev-server --config ./config/webpack/site/webpack.dev.conf.js",
    "release:site": "webpack --config ./config/webpack/site/webpack.publish.conf.js",
    "watch": "yarn build --watch",
    "start": "npm-run-all --parallel --print-label watch serve",
    "release": "NODE_ENV=production yarn build && lerna publish",
    "commit": "git add . && git-cz"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@vue/babel-preset-app": "^4.3.1",
    "@vue/compiler-sfc": "^3.0.0-beta.14",
    "babel-loader": "^8.1.0",
    "@slate-vue/babel-plugin-vue-next-jsx": "*",
    "commitizen": "^4.1.2",
    "css-loader": "^3.5.2",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^4.0.4",
    "lerna": "^3.20.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.7.2",
    "rollup-plugin-typescript2": "^0.27.0",
    "style-loader": "^1.1.4",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "vue-loader": "16.0.0-beta.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "cz-conventional-changelog": "^3.2.0",
    "image-extensions": "^1.1.0",
    "is-url": "^1.2.4",
    "prismjs": "^1.20.0",
    "slate-hyperscript": "^0.58.3",
    "vue": "3.0.0-beta.14",
    "vue-router": "4.0.0-alpha.12"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
